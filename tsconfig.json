{
  "compilerOptions": {
    "target": "ES2022",
    "module": "commonjs",
    "lib": [
      "ES2022"
    ],
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "types": [
      "jest",
      "node"
    ],
    "resolveJsonModule": true
  },
  "include": [
    "__tests__/**/*"
  ],
  "exclude": [
    "node_modules",
    "apps"
  ]
}